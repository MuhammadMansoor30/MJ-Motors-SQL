<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mj_motors.GuestController">
    <children>
        <Label layoutX="94.0" layoutY="55.0" text="IMPORTED Cars GUEST Section " textFill="#06808c">
            <font>
                <Font name="Engravers MT" size="32.0" />
            </font>
        </Label>
        <Button fx:id="purchaseLocal" layoutX="117.0" layoutY="241.0" mnemonicParsing="false" onAction="#checkoutImportedCars" text="Checkout Available IMPORTED Cars">
            <font>
                <Font name="Engravers MT" size="24.0" />
            </font>
        </Button>
        <Button fx:id="back" layoutX="396.0" layoutY="443.0" mnemonicParsing="false" onAction="#backToGuest" text="Back">
            <font>
                <Font name="Engravers MT" size="22.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
