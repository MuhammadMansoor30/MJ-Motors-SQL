<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mj_motors.GuestController">
    <children>
        <Label layoutX="121.0" layoutY="51.0" text="Local Cars Guest Section" textFill="#06808c">
            <font>
                <Font name="Engravers MT" size="36.0" />
            </font>
        </Label>
        <Button fx:id="purchaseLocal" layoutX="162.0" layoutY="252.0" mnemonicParsing="false" onAction="#checkoutLocalCars" text="Check Out Available Local Cars">
            <font>
                <Font name="Engravers MT" size="24.0" />
            </font>
        </Button>
        <Button fx:id="back" layoutX="400.0" layoutY="459.0" mnemonicParsing="false" onAction="#backToGuest" text="Back">
            <font>
                <Font name="Engravers MT" size="20.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
