<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mj_motors.AddNewLocalController">
   <children>
      <Label layoutX="233.0" layoutY="34.0" text="Add New Local Cars For Customers" textFill="#148f94">
         <font>
            <Font name="Lucida Sans Demibold Italic" size="24.0" />
         </font>
      </Label>
      <Label layoutX="61.0" layoutY="138.0" text="Car Name: " textFill="#05710a">
         <font>
            <Font name="Lucida Sans Demibold Italic" size="18.0" />
         </font>
      </Label>
      <Label fx:id="carTypeAdd" layoutX="64.0" layoutY="253.0" text="Car type:" textFill="#05710a">
         <font>
            <Font name="Lucida Sans Demibold Italic" size="18.0" />
         </font>
      </Label>
      <Label fx:id="carModelAdd" layoutX="464.0" layoutY="138.0" text="Car Model:" textFill="#05710a">
         <font>
            <Font name="Lucida Sans Demibold Italic" size="18.0" />
         </font>
      </Label>
      <Label layoutX="464.0" layoutY="252.0" text="Car Condition:" textFill="#05710a">
         <font>
            <Font name="Lucida Sans Demibold Italic" size="18.0" />
         </font>
      </Label>
      <Label layoutX="61.0" layoutY="366.0" text="Car Price:" textFill="#05710a">
         <font>
            <Font name="Lucida Sans Demibold Italic" size="18.0" />
         </font>
      </Label>
      <Button fx:id="addCar" layoutX="677.0" layoutY="464.0" mnemonicParsing="false" onAction="#addNewLocalCar" text="Add Car" textFill="#0b0bc1">
