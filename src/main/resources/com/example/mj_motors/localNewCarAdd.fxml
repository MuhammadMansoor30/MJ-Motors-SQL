            <Font name="Lucida Sans Demibold Italic" size="22.0" />
         </font>
      </Button>
      <ChoiceBox fx:id="chBoxType" layoutX="189.0" layoutY="252.0" prefHeight="26.0" prefWidth="180.0" />
      <TextField fx:id="carNameAdd" layoutX="189.0" layoutY="137.0" prefHeight="26.0" prefWidth="180.0" />
      <TextField fx:id="carPriceAdd" layoutX="190.0" layoutY="365.0" prefHeight="26.0" prefWidth="180.0" />
      <Spinner fx:id="spnModel" layoutX="610.0" layoutY="137.0" prefHeight="26.0" prefWidth="180.0" />
      <Spinner fx:id="spnCondition" layoutX="610.0" layoutY="251.0" prefHeight="26.0" prefWidth="180.0" />
      <Button fx:id="back" layoutX="124.0" layoutY="464.0" mnemonicParsing="false" onAction="#back" text="Back" textFill="#0b0bc1">
         <font>
            <Font name="Lucida Sans Demibold Italic" size="22.0" />
         </font>
      </Button>
