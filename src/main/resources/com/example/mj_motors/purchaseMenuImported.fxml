<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mj_motors.ImportedCarsController">
    <children>
        <Label layoutX="187.0" layoutY="52.0" text="Imported Cars Purchase Section" textFill="#69065f">
            <font>
                <Font name="Book Antiqua Bold" size="36.0" />
            </font>
        </Label>
        <Button fx:id="newCarSec" layoutX="76.0" layoutY="251.0" mnemonicParsing="false" onAction="#newCarsSection" text="New Cars Section" textFill="#056d76">
            <font>
                <Font name="Book Antiqua Bold Italic" size="24.0" />
            </font>
        </Button>
        <Button fx:id="OldCarSec" layoutX="619.0" layoutY="251.0" mnemonicParsing="false" onAction="#oldCarsSectionController" text="Old Cars Section" textFill="#056d76">
            <font>
                <Font name="Book Antiqua Bold Italic" size="24.0" />
            </font>
        </Button>
        <Button fx:id="back" layoutX="304.0" layoutY="434.0" mnemonicParsing="false" onAction="#backToImported" text="Back To Imported Menu" textFill="#a40101">
            <font>
                <Font name="Book Antiqua" size="24.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
