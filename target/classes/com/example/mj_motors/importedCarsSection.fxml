<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mj_motors.ImportedCarsController">
    <children>
        <Label layoutX="142.0" layoutY="38.0" text="IMPORTED Cars Section" textFill="#06808c">
            <font>
                <Font name="Engravers MT" size="36.0" />
            </font>
        </Label>
        <Button fx:id="purchaseLocal" layoutX="212.0" layoutY="160.0" mnemonicParsing="false" onAction="#purchaseImportedCars" text="Purchase IMPORTED Cars">
            <font>
                <Font name="Engravers MT" size="24.0" />
            </font>
        </Button>
        <Button fx:id="saleLocal" layoutX="262.0" layoutY="261.0" mnemonicParsing="false" onAction="#saleImportedCarsController" text="Sale IMPORTEDCars">
            <font>
                <Font name="Engravers MT" size="24.0" />
            </font>
        </Button>
        <Button fx:id="commissionLocal" layoutX="266.0" layoutY="367.0" mnemonicParsing="false" onAction="#placeCarForSaleController" text="Place Car For Sale">
            <font>
                <Font name="Engravers MT" size="24.0" />
            </font>
        </Button>
        <Button fx:id="back" layoutX="747.0" layoutY="479.0" mnemonicParsing="false" onAction="#back" text="Back">
            <font>
                <Font name="Engravers MT" size="18.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
