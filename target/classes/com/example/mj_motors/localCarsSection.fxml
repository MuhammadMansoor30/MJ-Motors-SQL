<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mj_motors.LocalCarsController">
   <children>
      <Label layoutX="192.0" layoutY="36.0" text="Local Cars Section" textFill="#06808c">
         <font>
            <Font name="Engravers MT" size="36.0" />
         </font>
      </Label>
      <Button fx:id="purchaseLocal" layoutX="245.0" layoutY="144.0" mnemonicParsing="false" onAction="#purchaseLocalCars" text="Purchase Local Cars">
         <font>
            <Font name="Engravers MT" size="24.0" />
         </font>
      </Button>
      <Button fx:id="saleLocal" layoutX="285.0" layoutY="261.0" mnemonicParsing="false" onAction="#saleLocalCarsController" text="Sale Local Cars">
         <font>
            <Font name="Engravers MT" size="24.0" />
         </font>
      </Button>
      <Button fx:id="commissionLocal" layoutX="273.0" layoutY="371.0" mnemonicParsing="false" onAction="#placeCarForSaleController" text="Place Car For Sale">
         <font>
            <Font name="Engravers MT" size="24.0" />
         </font>
      </Button>
      <Button fx:id="back" layoutX="751.0" layoutY="475.0" mnemonicParsing="false" onAction="#back" text="Back">
         <font>
            <Font name="Engravers MT" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
