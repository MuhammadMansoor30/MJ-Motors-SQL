<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="900.0" styleClass="root" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mj_motors.SignInController">
   <children>
      <Button layoutX="402.0" layoutY="428.0" mnemonicParsing="false" onAction="#login" text="Login" textFill="#190001">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button layoutX="624.0" layoutY="492.0" mnemonicParsing="false" onAction="#owner_Man" prefHeight="31.0" prefWidth="244.0" text="If you are an Owner/Manager! Click Here!" textFill="#290c8b">
         <font>
            <Font name="System Bold" size="11.0" />
         </font>
      </Button>
      <Button layoutX="774.0" layoutY="51.0" mnemonicParsing="false" onAction="#register" text="Register!" textFill="#8d0404">
         <font>
            <Font size="18.0" />
         </font></Button>
      <AnchorPane layoutX="116.0" layoutY="123.0" prefHeight="267.0" prefWidth="648.0" stylesheets="@../../../../../CssStyle/LoginStyle.css">
         <children>
            <Label layoutX="120.0" layoutY="55.0" text="UserName">
               <font>
                  <Font name="Goudy Old Style Bold" size="18.0" />
               </font></Label>
            <Label layoutX="125.0" layoutY="141.0" text="Password">
               <font>
                  <Font name="Goudy Old Style Bold" size="18.0" />
               </font></Label>
            <TextField fx:id="userLoginName" layoutX="257.0" layoutY="51.0" prefHeight="26.0" prefWidth="191.0" />
            <TextField fx:id="customerLoginId" layoutX="257.0" layoutY="219.0" prefHeight="26.0" prefWidth="191.0" />
            <Label layoutX="130.0" layoutY="221.0" prefHeight="18.0" prefWidth="59.0" text="Cs_ID">
               <font>
                  <Font name="Goudy Old Style Bold" size="18.0" />
               </font></Label>
            <PasswordField fx:id="userLoginPassword" layoutX="257.0" layoutY="139.0" prefHeight="26.0" prefWidth="191.0" />
         </children>
      </AnchorPane>
      <Label layoutX="309.0" layoutY="53.0" text="Login To Access The App">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
