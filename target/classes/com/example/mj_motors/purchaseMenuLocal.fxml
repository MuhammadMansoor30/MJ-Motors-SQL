<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mj_motors.LocalCarsController">
   <children>
      <Label layoutX="220.0" layoutY="52.0" text="Local Cars Purchase Section" textFill="#69065f">
         <font>
            <Font name="Book Antiqua Bold" size="36.0" />
         </font>
      </Label>
      <Button fx:id="newCarSec" layoutX="89.0" layoutY="255.0" mnemonicParsing="false" onAction="#newCarsSection" text="New Cars Section" textFill="#056d76">
         <font>
            <Font name="Book Antiqua Bold Italic" size="24.0" />
         </font>
      </Button>
      <Button fx:id="OldCarSec" layoutX="619.0" layoutY="255.0" mnemonicParsing="false" onAction="#oldCarsSectionController" text="Old Cars Section" textFill="#056d76">
         <font>
            <Font name="Book Antiqua Bold Italic" size="24.0" />
         </font>
      </Button>
      <Button fx:id="back" layoutX="325.0" layoutY="444.0" mnemonicParsing="false" onAction="#backToLocal" text="Back To Local Menu" textFill="#a40101">
         <font>
            <Font name="Book Antiqua" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
