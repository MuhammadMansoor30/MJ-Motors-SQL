<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mj_motors.SignInController">
   <children>
      <Label layoutX="268.0" layoutY="28.0" prefHeight="35.0" prefWidth="325.0" text="Provide Details To Register">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <AnchorPane layoutX="98.0" layoutY="71.0" prefHeight="362.0" prefWidth="666.0">
         <children>
            <Label layoutX="23.0" layoutY="20.0" prefHeight="19.0" prefWidth="57.0" text="Name">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label layoutX="23.0" layoutY="76.0" prefHeight="27.0" prefWidth="88.0" text="Password">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label layoutX="27.0" layoutY="135.0" prefHeight="27.0" prefWidth="70.0" text="Address">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label layoutX="23.0" layoutY="204.0" prefHeight="27.0" prefWidth="88.0" text="Phone No">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label layoutX="32.0" layoutY="271.0" prefHeight="27.0" prefWidth="50.0" text="CNIC">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="userName" layoutX="133.0" layoutY="17.0" prefHeight="26.0" prefWidth="262.0" />
            <TextField fx:id="userPassword" layoutX="133.0" layoutY="73.0" prefHeight="26.0" prefWidth="262.0" />
            <TextField fx:id="userAddress" layoutX="133.0" layoutY="132.0" prefHeight="26.0" prefWidth="262.0" />
            <TextField fx:id="userPhNo" layoutX="133.0" layoutY="201.0" prefHeight="26.0" prefWidth="262.0" />
            <TextField fx:id="userCNIC" layoutX="133.0" layoutY="272.0" prefHeight="26.0" prefWidth="262.0" />
         </children>
      </AnchorPane>
      <Button fx:id="register" layoutX="403.0" layoutY="452.0" mnemonicParsing="false" onAction="#pressRegister" text="Register" textFill="#087596">
         <font>
            <Font name="Bell MT Italic" size="22.0" />
         </font>
      </Button>
      <Button fx:id="guest" layoutX="659.0" layoutY="452.0" mnemonicParsing="false" onAction="#guestController" text="Enter as Guest" textAlignment="CENTER" textFill="#087596">
         <font>
            <Font name="Bell MT Italic" size="22.0" />
         </font>
      </Button>
      <Button layoutX="164.0" layoutY="452.0" mnemonicParsing="false" onAction="#back" text="Back" textFill="#087596">
         <font>
            <Font name="Bell MT Italic" size="22.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
